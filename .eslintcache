email, \r\n                    stats: data.stats\r\n                })\r\n                setCurrentSeries(data.lists.currentSeries);\r\n                setWatchedSeries(data.lists.watchedSeries);\r\n            } else {\r\n                console.log(\"No such document!\");\r\n            }\r\n        })\r\n        .catch(function(error) {\r\n            console.log(\"Error getting documents: \", error);\r\n        });\r\n    }\r\n\r\n    const getTrends = async () => {\r\n        console.log(\"Get Trends\");\r\n        const response = await fetch(`https://api.themoviedb.org/3/trending/tv/day?api_key=${API_KEY}`);\r\n        const myJSON = await response.json();\r\n        setTopTrends(myJSON.results);\r\n    }\r\n\r\n    return (\r\n        <AuthContext.Provider value={{ currentUser, userData, currentSeries, setCurrentSeries, watchedSeries, setWatchedSeries, topTrends }}>\r\n            {children}\r\n        </AuthContext.Provider>\r\n    )\r\n\r\n};\r\n\r\n",["94","95"],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\pages\\Home\\Home.js",[],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\components\\App.js",[],["96","97"],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\components\\Header\\Header.js",[],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\index.js",[],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\components\\List\\List.js",[],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\pages\\Home\\Trending\\Trending.js",[],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\components\\Tiles\\Tiles.js",[],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\components\\SearchBar\\SearchBar.js",[],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\components\\Authentication\\Login.js",[],"D:\\LOIC\\03_WEB\\02_JAVASCRIPT\\01_REACT\\my-watched-list\\src\\components\\Navigation\\LogNavigation.js",[],{"ruleId":"98","r